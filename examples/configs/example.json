{
	"loggers": [
		{
			"encoder": {
				"messageKey": "M",
				"levelKey": "L",
				"timeKey": "T",
				"nameKey": "N",
				"callerKey": "C"
			},
			"outputs": [
				{
					"minLevel": "debug",
					"maxLevel": "debug",
					"urls": [
						"./log/debug.log"
					]
				},
				{
					"minLevel": "info",
					"maxLevel": "fatal",
					"urls": [
						"./log/info.log"
					]
				},
				{
					"minLevel": "warn",
					"maxLevel": "fatal",
					"urls": [
						"./log/warn.log"
					]
				},
				{
					"minLevel": "error",
					"maxLevel": "fatal",
					"urls": [
						"./log/error.log"
					]
				},
				{
					"minLevel": "panic",
					"maxLevel": "fatal",
					"urls": [
						"./log/fatal.log"
					]
				}
			]
		},
		{
			"name": "access",
			"encoder": {
				"messageKey": "M",
				"levelKey": "L",
				"timeKey": "T",
				"nameKey": "N",
				"callerKey": "C"
			},
			"outputs": [
				{
					"minLevel": "debug",
					"maxLevel": "fatal",
					"urls": [
						"./log/access.log"
					]
				}
			]
		}
	]
}